{
  "name": "plover-arttemplate",
  "version": "2.0.0",
  "description": "【模板引擎插件】接入[artTemplate](https://github.com/aui/artTemplate)",
  "main": "lib/index",
  "scripts": {
    "lint": "eslint lib test",
    "test": "mocha --require should --reporter spec --recursive --bail --check-leaks test/",
    "cov": "istanbul cover _mocha -- --require should --recursive --bail --check-leaks test/ -R spec",
    "travis": "istanbul cover _mocha --report lcovonly -- --require should --recursive --bail --check-leaks test/ -R spec",
    "ci": "npm run lint && npm run test && npm run travis",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -o CHANGELOG.md -s"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "art-template-for-plover": "~3.5.0",
    "escape-html": "~1.0.3",
    "is-type-of": "~1.0.0",
    "plover-util": "~1.4.0"
  },
  "devDependencies": {
    "co": "~4.6.0",
    "conventional-changelog-cli": "~1.2.0",
    "eslint": "~3.6.1",
    "eslint-config-bce": "~1.3.0",
    "istanbul": "~0.4.1",
    "mocha": "~3.1.0",
    "plover": "~2.6.1",
    "should": "~11.1.0",
    "supertest": "~2.0.0"
  },
  "plover": {
    "plugin": "lib/plugin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plover-modules/plover-arttemplate.git"
  },
  "author": "bencode@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plover-modules/plover-arttemplate/issues"
  },
  "homepage": "https://github.com/plover-modules/plover-arttemplate#readme"
}
